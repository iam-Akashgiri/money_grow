<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="vendor_remarks_view_tree" model="ir.ui.view">
            <field name="name">Vendor Remarks</field>
            <field name="model">vendor.remarks</field>
            <field name="arch" type="xml">
                <list string="Vendor Remarks" editable="bottom">
                    <field name="app_id"/>

                    <field name="name"/>
                </list>
            </field>
        </record>

        <record id="vendor_remarks_action" model="ir.actions.act_window">
            <field name="name">Vendor Remarks</field>
            <field name="res_model">vendor.remarks</field>
            <field name="view_mode">list</field>
        </record>

        <menuitem
                id="menu_vendor_remarks"
                name="Vendor Remarks"
                parent="mis_modification.mis_main_menu_config_root"
                action="vendor_remarks_action"
                sequence="30"/>


    </data>
</odoo>
